const http = require('http');
const server = http.createServer((req,res)=> {
    if (req.url === '/') {
        res.write('Welcome')
        res.end()
    } else if (req.url === '/about') {
        res.write('Our story')
        res.end()
    } else
    res.write('opps')
    res.end()
})

server.listen(5000)